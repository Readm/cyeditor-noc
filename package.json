{
    "name": "cyeditor",
    "version": "1.0.3",
    "private": false,
    "description": "CyEditor, A visual flow chart editor based on cytoscape.js",
    "main": "dist/cyeditor.umd.js",
    "jsdelivr": "dist/cytoscape.umd.min.js",
    "keywords": [
        "cyeditor",
        "flowchart",
        "flow chart",
        "flow editor",
        "process editor",
        "flow chart editor",
        "vue flow chart",
        "cytoscape editor",
        "javascript flow",
        "graph editor",
        "diagram"
    ],
    "scripts": {
        "serve": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service serve examples/main.js",
        "build": "vue-cli-service build --target lib --name cyeditor src/index.js",
        "lint": "vue-cli-service lint --fix",
        "docs:dev": "vuepress dev docs",
        "docs:build": "vuepress build docs",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "precommit": "lint-staged",
        "deploy": "./deploy.sh"
    },
    "husky": {
        "hooks": {
            "postcommit": "git update-index --again",
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*": [
            "eslint --fix",
            "git add"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/demonray/cyeditor.git"
    },
    "bugs": {
        "url": "https://github.com/demonray/cyeditor/issues"
    },
    "author": "ryanlei <leiqirong@gmail.com>",
    "license": "ISC",
    "dependencies": {
        "cytoscape": "^3.7.0",
        "vue-json-viewer": "^2.2.22"
    },
    "devDependencies": {
        "@babel/core": "^7.12.3",
        "@babel/preset-env": "^7.12.1",
        "@vue/cli-plugin-babel": "^3.5.1",
        "@vue/cli-plugin-eslint": "^3.5.1",
        "@vue/cli-service": "^3.5.1",
        "@vue/eslint-config-standard": "^4.0.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^26.6.3",
        "eslint": "^5.15.3",
        "eslint-plugin-vue": "^5.2.2",
        "jest": "^26.6.3",
        "jest-environment-jsdom": "^26.6.2",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2",
        "vue": "^2.6.10",
        "vue-template-compiler": "^2.6.10",
        "vuepress": "^1.0.2",
        "webpack": ">=4 < 4.29"
    }
}
